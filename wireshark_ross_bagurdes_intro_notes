tshark -D
tshark -i 1 #capture on interface 1 
tshark -i 1 -w myCaptureFileName.pcap -F pcapng 
#end capture by ctrl-C

#onlinux
tcpdump -D
tcpdump -i 3 # capture on interface 3
tcpdump -i3 -w output.pcap
